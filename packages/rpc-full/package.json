{
  "name": "@wranggle/rpc",
  "version": "0.3.2",
  "description": "Call functions that live and run in a different JavaScript window/process",
  "main": "dist/wranggle-rpc.cjs.js",
  "browser": "dist/wranggle-rpc.umd.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "bundle": "run-s clean tsc rollup tsc:unxkcd-types",
    "build": "npm run bundle",
    "rollup": "rollup -c",
    "tsc": "tsc",
    "tsc:unxkcd-types": "./bin/unxkcd-types.js",
    "clean:dist": "rimraf dist/",
    "clean:out": "rimraf out/",
    "clean": "run-p clean:*"
  },
  "author": "Jeff Ferber",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wranggle/rpc.git"
  },
  "bugs": {
    "url": "https://github.com/wranggle/rpc/issues"
  },
  "dependencies": {
    "@wranggle/rpc-browser-extension-transport": "^0.3.2",
    "@wranggle/rpc-core": "^0.3.2",
    "@wranggle/rpc-electron-transport": "^0.3.1",
    "@wranggle/rpc-ipc-transport": "^0.3.2",
    "@wranggle/rpc-post-message-transport": "^0.3.2",
    "@wranggle/rpc-relay": "^0.3.2",
    "@wranggle/rpc-websocket-transport": "^0.3.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.19.3",
    "typescript": "^3.3.3333"
  },
  "keywords": [
    "wranggle-rpc",
    "rpc",
    "remote-procedure-call",
    "ipc",
    "iframe",
    "browser-extension",
    "electron"
  ],
  "homepage": "https://github.com/wranggle/rpc",
  "license": "Apache-2.0"
}
