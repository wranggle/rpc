  "keywords": [
    "wranggle-rpc",
    "rpc",
    "wranggle-rpc-transport"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wranggle/rpc.git"
  },
  "bugs": {
    "url": "https://github.com/wranggle/rpc/issues"
  },
  "dependencies": {
    "@wranggle/rpc-core": "^0.3.2",
    "kvid": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^11.9.5",
    "ts-jest": "^24.0.0"
  }
